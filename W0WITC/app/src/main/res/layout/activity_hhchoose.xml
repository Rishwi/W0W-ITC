<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tech.rishwibinnu.w0w_itc.hhchoose">

    <Button
        android:id="@+id/hhfirstvisit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/first_visit"
        android:background="#c6c0c6"
        android:textColor="#0612f7"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginTop="10dp"/>

    <Button
        android:id="@+id/hhfollowup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/followup"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:background="#c6c0c6"
        android:textColor="#0612f7"
        android:layout_marginTop="80dp"/>

</RelativeLayout>
