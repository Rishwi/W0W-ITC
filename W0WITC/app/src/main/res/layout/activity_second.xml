<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tech.rishwibinnu.w0w_itc.SecondActivity">


    <ImageButton
        android:id="@+id/household"
        android:layout_width="190dp"
        android:layout_height="200dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:scaleType="fitCenter"
        android:src="@drawable/hh" />

    <ImageButton
        android:id="@+id/school"
        android:layout_width="190dp"
        android:layout_height="200dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:scaleType="fitCenter"
        android:src="@drawable/school1" />

    <ImageButton
        android:id="@+id/corporate"
        android:layout_width="190dp"
        android:layout_height="200dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/household"
        android:scaleType="fitCenter"
        android:src="@drawable/corporate" />

    <ImageButton
        android:id="@+id/hub"
        android:layout_width="190dp"
        android:layout_height="200dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/school"
        android:scaleType="centerCrop"
        android:src="@drawable/hub" />



</RelativeLayout>
